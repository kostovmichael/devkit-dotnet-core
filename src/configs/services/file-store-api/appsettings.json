{
    "AllowedHosts": "devkit.file-store",
    "APIDefinition": {
        "Name": "Devkit File Store API",
        "Description": "File Store API stores user uploaded files.",
        "Version": "v1"
    },
    "ConsulConfiguration": {
        "ConsulHost": "http://devkit.consul:8500",
        "ServiceName": "file-store-api",
        "DataCenter": "dc1",
        "ServiceId": "devkit.file-store",
        "Port": "80",
        "Tags": [
            "devkit",
            "file-store",
            "internal"
